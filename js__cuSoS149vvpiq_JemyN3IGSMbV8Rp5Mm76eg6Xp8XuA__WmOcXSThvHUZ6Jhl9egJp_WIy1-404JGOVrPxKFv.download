(function ($) {
  Drupal.behaviors.igcommerce_utility_products = {
    attach: function (context, settings) {

      // Any js that needs to be instantiated and is above the fold should be 
      // moved out of this file and into 
      // sites/all/modules/custom/igcommerce_utility/modules/igcommerce_utility_products/js/igcommerce_utility_products_async.js
      $(document).ready(function() {
        $('a.fluke-product-display-twister.active:first').closest('div.fluke-product-display-twister-item').addClass('active');

        // tiny helper function to add breakpoints
        function getGridSize() {
          return (window.innerWidth < 576) ? 2 :(window.innerWidth < 1033) ? 3 : 4;
        }

        // If pricespider for a model fails, reveal a link to the online store if one exists
        $('.product-model a.hide.buy-now-cta').each(
          function() {
            var model = $(this).closest('.product-model');

            var ps = $(model).find('.ps-widget');

            if ($(ps).hasClass('ps-no-sku')) {
              $(this).removeClass('hide');
            } else if(!$(ps).hasClass('ps-enabled')) {
                setTimeout(modelPriceSpiderCheck, 1000, ps, this);
            }
          }
        );

        /**
         * Ecommerce Countdown til sale ends
         * If an ecommerce product has a sale end date set, build the countdown timer above the CTA section.
         */
        
        // Only build the countdown if the countdown message container exists.
        if ($('#ecomm-countdown-message').length > 0) {
        // Get the sale end date, found from ECM models model->ecommerce_sale_end. Updated variable name, old one had a value stuck there for some reason (04.02.21)
        var sale_end_date        = settings.igcommerce_sale_price.sale_end_count;
        countDownToTime(sale_end_date, 'ecomm-sale-countdown'); // Gets the ID of the countdown timer, to evalute the day | hour | minute | second til sale end
        
        // Calculate the time til the sale is ended
        function countDownToTime(countTo, id) {
          countTo = new Date(countTo).getTime();
          var now = new Date(),
              countTo = new Date(countTo),
              timeDifference = (countTo - now),
              currentDateIso = moment(now).toISOString(),
              saleEndIso = moment(countTo).toISOString();
              
          // If the current time is before the sale end date, remove the countdown time. 
          // TODO: This should also be sync'd to both shopify + cloudflare via ECM. Currently it is not the case, 
          // and is a feature that is being developed. 
          if (currentDateIso < saleEndIso) {
            var secondsInADay = 60 * 60 * 1000 * 24,
            secondsInAHour = 60 * 60 * 1000;
      
            days = Math.floor(timeDifference / (secondsInADay) * 1);
            hours = Math.floor((timeDifference % (secondsInADay)) / (secondsInAHour) * 1);
            mins = Math.floor(((timeDifference % (secondsInADay)) % (secondsInAHour)) / (60 * 1000) * 1);
            secs = Math.floor((((timeDifference % (secondsInADay)) % (secondsInAHour)) % (60 * 1000)) / 1000 * 1);
      
            var countdownId = document.getElementById(id);
            countdownId.getElementsByClassName('days')[0].innerHTML = days;
            countdownId.getElementsByClassName('hours')[0].innerHTML = hours;
            countdownId.getElementsByClassName('minutes')[0].innerHTML = mins;
            countdownId.getElementsByClassName('seconds')[0].innerHTML = secs;
      
            clearTimeout(countDownToTime.interval);
            countDownToTime.interval = setTimeout(function(){ countDownToTime(countTo, id); },1000);
          }
          // If the countodown is over, remove the javascript. This should only be if the countdown is over when the customer is on the page.
          // If the page is loaded when the sale is already expired, no timer should appear at all.
          else {
            $( "#ecomm-sale-countdown" ).remove();
          }
            
        }
        }
        // If a product has variant, we will display a `see all models` link under key features. This link with scroll to, and open the model tab
        $( "#fluke-product-display-see-models" ).click(function() {
          var gridSize = getGridSize();
            // Mobile
            if(gridSize <= 3) {
              $('#models-tab-collapse').collapse('show');
            } else {
            // Desktop
              $('#myTab a[href="#models-tab"]').tab('show');
              $('#models-accordion #models-title .collapsed').click();
            }
            // Scrolling for both mobile and desktop.
            $(window).scrollTo('#models-label', {
              axis: 'y',
              duration: 800
          });
        });  

        // When clicking the manual link, this will open the tab and scroll to the section.
        $( "#fluke-product-display-manuals" ).click(function() {
          var gridSize = getGridSize();
          // Mobile
          if (gridSize <= 3) {
            $('#resources-tab-collapse').collapse('show');
          } else {
          // Desktop
            $('#myTab a[href="#resources-tab"]').tab('show');
            $('#resources-accordion #resources-manuals-title .collapsed').click();
          }
          // Scrolling for both mobile and desktop.
          $(window).scrollTo('#resources-label', {
            axis: 'y',
            duration: 800
          });
        });
        // Remove the ribbon for all but first flex slide
        $( ".flex-control-nav li:first" ).click(function() {
          $('#fluke-product-atf-slides div.ribbon').show();
          // #ig-product-flexslider . ribbon
        });
        $( ".flex-control-nav li:not(:first)" ).click(function() {
          $('#fluke-product-atf-slides div.ribbon').hide();
        });
        // By default, the 'Product Overview' tab is hardcoded as active, so that
        // desktop users don't see a weird layout while waiting for tab logic to
        // apply. Some tabs don't have an overview tab, so this logic will set
        // the first tab to display after page load.
        // For mobile devices, we want to load the page with accordions closed.
        // We strip off the tags that are used to hardcode 'active' before the 
        // accordion is instantiated.
        // When colorbox opens a model, the browser perceives it as an 'onload' 
        // event. This caused the tabs to reset when a modal was opened, so we
        // added the colorbox display check logic. -RRN 9-34-18
        if (getGridSize() == 4 && $('#colorbox').css('display') !== 'block') {
          $('#myTab a:first').tab('show');
        } else {
          $('#overview-tab').removeClass('active in');
          $('.overview-list-item').removeClass('active');
        }
        
        // This is mostly for Fluke reviewers, when they resize the window it resets to the first tab.
        $(window).on("resize", function() {
          if (getGridSize() == 4) {
            $('#myTab a:first').tab('show');
          }
        });

        // Instantiates tab-collapse for mobile and tablet devices
        $('#myTab').tabCollapse({
            tabsClass: 'hidden-tabs',
            accordionClass: 'visible-accordion'
        });

        // On mobile and tablet, some resources and related products accordions should be fully open.
        // We're borrowing the gridSize() function used for slideshows to get the screen width.
        if($('#resources-accordion').length > 0) {
          var gridSize = getGridSize();
          //console.log(gridSize);
          if (gridSize <= 3) {
            $('#myTab a:first').tab('show'); // CWI3409: Please default the product overview tab on the PDP page mobile version to open.
            $('#resources-accordion .collapse').collapse('show');
          } else {
            $('#resources-accordion .collapse').collapse('hide');
          }
        }
        if ($('#accessory-accordion').length > 0) {
          var gridSize = getGridSize();
          if(gridSize <= 3) {
            $('#accessory-accordion .collapse').collapse('show');
          } else {
            $('#accessory-accordion .collapse').collapse('hide');
          }
        }
        if ($('#accessories-compatible-products-accordion').length > 0) {
          var gridSize = getGridSize();
          if (gridSize <= 3) {
            $('#accessories-compatible-products-accordion .collapse').collapse('show');
          } else {
            $('#accessories-compatible-products-accordion .collapse').collapse('hide');
          }
        }

        // this sets the accordions on page resize
        $(window).on("resize", function(){
          if ($('#resources-accordion').length > 0) {
            var gridSize = getGridSize();
            //console.log(gridSize);
            if(gridSize <= 3) {
              $('#resources-accordion .collapse').collapse('show');
            } else {
              $('#resources-accordion .collapse').collapse('hide');
            }
          }
          if($('#accessory-accordion').length > 0) {
            var gridSize = getGridSize();
            if (gridSize <= 3) {
              $('#accessory-accordion .collapse').collapse('show');
            } else {
              $('#accessory-accordion .collapse').collapse('hide');
            }
          }
          if ($('#accessories-compatible-products-accordion').length > 0) {
            var gridSize = getGridSize();
            if (gridSize <= 3) {
              $('#accessories-compatible-products-accordion .collapse').collapse('show');
            } else {
              $('#accessories-compatible-products-accordion .collapse').collapse('hide');
            }
          }
        });
      });


      // Get first datasheet from Resources section
      var first_datasheet = $('#datasheets-ul').first().find('a').attr('href');
      if ($('#datasheets-ul').length) {
        $('#specification_link').attr('href',first_datasheet);
      } else {
        $('#specification_link').hide();
      }

      // Translations for the 'read more' and 'read less' buttons
      var readMoreTranslated = Drupal.t('Read more');
      var readLessTranslated = Drupal.t('Read less');
      
      // Add Read more& Less for Key Feature
      // uses the readmore.js library
      // https://jedfoster.com/Readmore.js/
      $('#keyfeature_content').readmore({
        speed: 150,
        moreLink: '<a class="event-features" href="#">' + readMoreTranslated + '</a>',
        lessLink: '<a href="#.">' + readLessTranslated + '</a>',
        collapsedHeight: 150,
